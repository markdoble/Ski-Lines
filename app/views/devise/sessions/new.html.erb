	<div class="container content">
				<div class="col-lg-12 col-md-12 col-sm-12 hidden-xs">
					<br /><br /><br />
				</div>
			 	<div class="row">
				  <div class="col-lg-4 col-md-4 col-sm-3 hidden-xs">
				  </div>
					<div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
						 <div class="devise_forms_background">
							<br />
							 <div class="merchant_form_headings">
							 <h2>Log In:</h2>
						 	</div>
								<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
							  <div class="field">
								  <div class="devise_session_label">
							    <%= f.label :email, hidden: true  %>
							      </div>
							    <%= f.email_field :email, class: "form-control input-lg", autofocus: true, placeholder: "Email" %>
							  </div>
							  <div class="field">
								  <div class="devise_session_label">
							    <%= f.label :password, hidden: true %>
								  </div>
							    <%= f.password_field :password, class: "form-control input-lg", autocomplete: "off", placeholder: "Password" %>
							  </div>
				        <div class="devise_session_label">
								  <div class="actions">
										<br />
								    <%= f.submit "Go!", class: "btn btn-large btn-primary registration_button"%>
								  </div>
							  </div>
								<% end %><br />
						</div><br /><br />
						<div class="devise_forms_background">
							<br />
							<h4>Don't Have an Account?</h4>
							<%= link_to "Sign Up", new_user_registration_path, class: "btn-primary btn bt-large registration_button"%>
							<br /><br />
						</div>
					</div>
					<div class="col-lg-4 col-md-4 col-sm-3 hidden-xs">
				  </div>
			</div>
	 </div>
