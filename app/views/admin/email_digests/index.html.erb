

<div class="admin_performance">
  <br /><br />
    <table style="width:100%">
        <tr>
          <th>#</th>
          <th>First Name</th>
    			<th>Last Name</th>
    			<th>Email</th>
          <th>Status</th>
    			<th>Created At</th>
    		</tr>
        <% @subscribers.each_with_index do |f, index| %>
        <tr>
          <td><%= index+1 %></td>
          <td><%= f.first_name %></td>
    			<td><%= f.last_name %></td>
    			<td><%= f.email %></td>
          <td><%= f.status %></td>
          <td> <%= f.created_at.strftime("%B %e, %Y") %> </td>
    		</tr>
        <% end %>
    </table>
    <br /><br />
    <br /><br />
</div>
