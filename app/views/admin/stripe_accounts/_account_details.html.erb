
<div class="form_heading col-xs-12">
  <h4>Account Representative Details:</h4>
  <br /><br />
</div>
<div class="row">
  <div class="left_label col-lg-3 col-md-3 col-sm-4 col-xs-12">
    First Name:
  </div>
  <div class="col-lg-7 col-md-7 col-sm-6 col-xs-12">
    <%= @account_first_name %>
  </div>
</div>
<div class="row">
  <div class="left_label col-lg-3 col-md-3 col-sm-4 col-xs-12">
    Last Name:
  </div>
  <div class="col-lg-7 col-md-7 col-sm-6 col-xs-12">
    <%= @account_last_name %>
  </div>
</div>
<div class="row">
  <div class="left_label col-lg-3 col-md-3 col-sm-4 col-xs-12">
    Date of Birth:
  </div>
  <div class="col-lg-9 col-md-9 col-sm-8 col-xs-12">
    <%= @dob_month %> <%= @dob_day %>, <%= @dob_year %>
  </div>
</div>
